<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <version>$version$</version>
    <authors>safestak-keith</authors>
    <owners>safestak-keith</owners>
    <id>AzureBusDepot</id>
    <title>Azure Bus Depot</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/keithnut/AzureBusDepot</projectUrl>
    <description>
      A compact, opinioned but extensible .Net Standard SDK for Azure Service Bus which aims to tackle the common cross-cutting concerns around handling typed messages.
