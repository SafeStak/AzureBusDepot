        <dependency id="Microsoft.ApplicationInsights" version="2.9.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Azure.ServiceBus" version="4.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Logging" version="2.2.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <copyright></copyright>
  </metadata>
  <files>
    <file src="bin\$configuration$\netstandard2.0\AzureBusDepot*" target="lib\netstandard2.0" />
  </files>
</package>